{
  "name": "vanilla-JS-library-boilerplate",
  "version": "0.0.1",
  "description": "A simple boilerplate for creating a JavaScript library that is framework agnostic",
  "main": "lib/index.min.js",
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "start": "webpack-dev-server --port 8031 --open --config webpack.dev.js",
    "test": "jest",
    "lint": "eslint *.js src/*.js src/*/*.js ",
    "precommit": "yarn build && yarn lint && yarn test",
    "prepush": "yarn lint && yarn test"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.4.2",
    "babel-loader": "^8.0.0",
    "clean-webpack-plugin": "^0.1.19",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.1.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.5",
    "jest": "^25.4.0",
    "prettier": "^2.0.5",
    "regenerator-runtime": "^0.11.1",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^4.5.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.1.3",
    "webpack-merge": "^4.1.2"
  },
  "jest": {
    "verbose": true,
    "testURL": "http://localhost/"
  },
  "author": "James Hall <eljamez@gmail.com> (http://jamescript.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eljamez/Vanilla-JS-Library-Boilerplate/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eljamez/Vanilla-JS-Library-Boilerplate"
  },
  "homepage": "https://github.com/eljamez/Vanilla-JS-Library-Boilerplate#readme",
  "keywords": [
    "react-component",
    "musical-typing",
    "react-keyboard",
    "react"
  ]
}
